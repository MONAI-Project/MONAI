      "SignalRandShift",
      "SignalResample",
      "SignalRandAddSine",
      "SignalRandAddSquarePulse",
      "SignalRandAddGaussianNoise",
      "SignalRandAddSinePartial",
      "SignalRandAddSquarePulsePartial",
      "SignalNormalize",
      "SignalStandardize",
      "SignalZeroPad",
      "SignalFillEmpty",
      "SignalRemoveFrequency",
      "SignalShortTimeFourier",
      "SignalContinousWavelet",

